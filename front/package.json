{
  "name": "posting-app-frontend",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@stripe/stripe-js": "^1.54.0",
    "@types/node": "^16.18.0",
    "@types/react": "^17.0.52",
    "@types/react-dom": "^17.0.18",
    "axios": "^1.10.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.8.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run api:generate && panda && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "panda": "panda codegen",
    "panda:watch": "panda --watch",
    "api:generate": "orval",
    "api:watch": "orval --watch",
    "dev:setup": "npm run api:generate && npm run panda",
    "prestart": "npm run dev:setup"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "src/generated/**/*"
        ],
        "rules": {
          "no-undef": "off",
          "no-redeclare": "off",
          "@typescript-eslint/no-redeclare": "off",
          "@typescript-eslint/no-unused-vars": "off"
        }
      }
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "^axios$": "axios/dist/node/axios.cjs"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(axios)/)"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@orval/axios": "^7.10.0",
    "@orval/core": "^7.10.0",
    "@pandacss/dev": "^0.54.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@types/jest": "^29.5.0",
    "@types/minimatch": "^3.0.5",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^7.32.0",
    "orval": "^7.10.0",
    "prettier": "^3.1.0",
    "react-scripts": "^0.0.0"
  }
}
